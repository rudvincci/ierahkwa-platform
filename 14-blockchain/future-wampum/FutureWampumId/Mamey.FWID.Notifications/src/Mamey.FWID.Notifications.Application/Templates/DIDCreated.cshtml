@model Mamey.FWID.Notifications.Application.Templates.Models.DIDCreatedDto
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>DID Created</title>
</head>
<body>
    <h1>Your Decentralized Identifier (DID) Has Been Created</h1>
    <p>Hello @Model.Name.FirstName @Model.Name.LastName,</p>
    <p>Your DID has been successfully created:</p>
    <p><strong>@Model.DidString</strong></p>
    <p>You can now use this DID for decentralized identity operations.</p>
</body>
</html>







