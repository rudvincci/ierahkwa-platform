    <ProjectReference Include="..\..\..\Mamey.Auth.Distributed\src\Mamey.Auth.Distributed\Mamey.Auth.Distributed.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Authentication" Version="2.3.0" />
