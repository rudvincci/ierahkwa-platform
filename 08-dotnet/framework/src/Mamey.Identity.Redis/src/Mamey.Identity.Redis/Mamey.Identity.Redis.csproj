<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageId>Mamey.Identity.Redis</PackageId>
    <Version>2.0.5</Version>
    <Authors>Mamey.io</Authors>
    <Description>Redis integration for Mamey.Identity token caching and blacklisting</Description>
    <PackageProjectUrl>https://github.com/Mamey-io/Mamey</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Mamey-io/Mamey</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>identity;redis;caching;authentication;tokens</PackageTags>
    <AssemblyName>Mamey.Identity.Redis</AssemblyName>
    <Title>Mamey.Identity.Redis</Title>
    <PackageLicenseExpression>Proprietary</PackageLicenseExpression>
    <Copyright>Copyright (c) 2025 Mamey.io</Copyright>
    <Company>Mamey.io</Company>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>Mamey Framework 2.0 release</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="StackExchange.Redis" Version="2.8.16" />
    <PackageReference Include="Microsoft.Extensions.Caching.StackExchangeRedis" Version="9.0.11" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="9.0.11" />
    <PackageReference Include="Microsoft.Extensions.Options" Version="9.0.11" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="9.0.11" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\Mamey.Identity.Core\src\Mamey.Identity.Core\Mamey.Identity.Core.csproj" />
  </ItemGroup>

</Project>
