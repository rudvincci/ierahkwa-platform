<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>Mamey.Blockchain.Swap - .NET library for DEX swap operations, AMM, and routing</Description>
    <Authors>Mamey.io</Authors>
    <LangVersion>Latest</LangVersion>
    <AssemblyName>Mamey.Blockchain.Swap</AssemblyName>
    <Title>Mamey.Blockchain.Swap</Title>
    <PackageId>Mamey.Blockchain.Swap</PackageId>
    <PackageTags>Mamey;Blockchain;Swap;DEX;AMM</PackageTags>
    <Version>2.0.0</Version>
    <PackageLicenseExpression>Proprietary</PackageLicenseExpression>
    <Copyright>Copyright (c) 2025 Mamey.io</Copyright>
    <Company>Mamey.io</Company>
    <PackageProjectUrl>https://github.com/Mamey-io/Mamey</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Mamey-io/Mamey</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>Mamey Framework 2.0 release - DEX swap functionality</PackageReleaseNotes>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="9.0.11" />
  </ItemGroup>
  
  <ItemGroup>
    <ProjectReference Include="..\..\..\Mamey\src\Mamey\Mamey.csproj" />
    <ProjectReference Include="..\..\..\Mamey.Blockchain.Node\src\Mamey.Blockchain.Node\Mamey.Blockchain.Node.csproj" />
  </ItemGroup>
</Project>
