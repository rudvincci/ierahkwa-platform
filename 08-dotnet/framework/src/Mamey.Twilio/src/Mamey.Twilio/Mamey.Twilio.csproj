<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>Mamey.Twilio</Description>
    <Authors>Mamey.io</Authors>
    <LangVersion>Latest</LangVersion>
    <AssemblyName>Mamey.Twilio</AssemblyName>
    <Title>Mamey.Twilio</Title>
    <PackageId>Mamey.Twilio</PackageId>
    <PackageTags>Mamey.Twilio</PackageTags>
    <Version>2.0.5</Version>
    <PackageLicenseExpression>Proprietary</PackageLicenseExpression>
    <Copyright>Copyright (c) 2025 Mamey.io</Copyright>
    <Company>Mamey.io</Company>
    <PackageProjectUrl>https://github.com/Mamey-io/Mamey</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Mamey-io/Mamey</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>Mamey Framework 2.0 release</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Configuration\" />
    <None Remove="Twilio.AspNet.Core" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Configuration\" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\..\Mamey\src\Mamey\Mamey.csproj" />

  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Twilio.AspNet.Core" Version="8.1.1" />
  </ItemGroup>
</Project>
