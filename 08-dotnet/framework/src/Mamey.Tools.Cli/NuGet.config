<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!--
    CLI-local NuGet config:
    - Avoid hard dependency on the local docker feed (localhost:4000) so developers can build even when the container is down.
    - Use nuget.org by default; repo projects are referenced via ProjectReference.
  -->
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
</configuration>




