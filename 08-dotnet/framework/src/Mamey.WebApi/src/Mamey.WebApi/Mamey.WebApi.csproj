<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>Mamey.WebApi</Description>
    <Authors>Mamey.io</Authors>
    <LangVersion>Latest</LangVersion>
    <AssemblyName>Mamey.WebApi</AssemblyName>
    <Title>Mamey.WebApi</Title>
    <PackageId>Mamey.WebApi</PackageId>
    <PackageTags>Mamey.WebApi</PackageTags>
    <Version>2.0.5</Version>
    <PackageLicenseExpression>Proprietary</PackageLicenseExpression>
    <Copyright>Copyright (c) 2025 Mamey.io</Copyright>
    <Company>Mamey.io</Company>
    <PackageProjectUrl>https://github.com/Mamey-io/Mamey</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Mamey-io/Mamey</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>Mamey Framework 2.0 release</PackageReleaseNotes>
  </PropertyGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\..\Mamey.Net\src\Mamey.Net\Mamey.Net.csproj" />

  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Open.Serialization.Json.System" Version="3.0.0" />
    <PackageReference Include="Open.Serialization.Json.Utf8Json" Version="3.0.0" />
  </ItemGroup>
</Project>
