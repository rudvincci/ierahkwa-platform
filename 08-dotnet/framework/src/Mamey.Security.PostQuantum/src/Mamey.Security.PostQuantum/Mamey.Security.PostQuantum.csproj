<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>Mamey.Security.PostQuantum</Description>
    <Authors>Mamey.io</Authors>
    <LangVersion>Latest</LangVersion>
    <AssemblyName>Mamey.Security.PostQuantum</AssemblyName>
    <Title>Mamey.Security.PostQuantum</Title>
    <PackageId>Mamey.Security.PostQuantum</PackageId>
    <PackageTags>Mamey.Security;PostQuantum;PQC</PackageTags>
    <Version>2.0.5</Version>
    <PackageLicenseExpression>Proprietary</PackageLicenseExpression>
    <Copyright>Copyright (c) 2025 Mamey.io</Copyright>
    <Company>Mamey.io</Company>
    <PackageProjectUrl>https://github.com/Mamey-io/Mamey</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Mamey-io/Mamey</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>Mamey Framework 2.0 - Post-Quantum Security foundation</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="9.0.11" />
    <PackageReference Include="Portable.BouncyCastle" Version="1.9.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\Mamey\src\Mamey\Mamey.csproj" />
  </ItemGroup>

</Project>



