<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <RazorLangVersion>9.0</RazorLangVersion>
  </PropertyGroup>

  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="9.0.11" />
    <PackageReference Include="Microsoft.AspNetCore.SignalR.Client" Version="9.0.11" />
    <PackageReference Include="Mamey.BlazorWasm" Version="2.0.*" />
  </ItemGroup>

  <ItemGroup>
    <!-- Reference BlazorWasm micro-frontend projects -->
    <!-- Add project references here, e.g.:
    <ProjectReference Include="..\..\Mamey.Domain.Service1.BlazorWasm\Mamey.Domain.Service1.BlazorWasm.csproj" />
    <ProjectReference Include="..\..\Mamey.Domain.Service2.BlazorWasm\Mamey.Domain.Service2.BlazorWasm.csproj" />
    -->
  </ItemGroup>

</Project>











