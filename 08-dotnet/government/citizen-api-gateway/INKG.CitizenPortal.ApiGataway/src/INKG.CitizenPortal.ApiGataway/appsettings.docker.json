{
    "app": {
        "name": "FBA - Bank API",
        "service": "ink-api",
        "version": "1"
    },
    "jaeger": {
        "enabled": true,
        "serviceName": "ink-api",
        "udpHost": "jaeger",
        "udpPort": 6831,
        "maxPacketSize": 0,
        "sampler": "const"
    },
    "logger": {
        "console": {
            "enabled": true
        },
        "elk": {
            "enabled": false,
            "url": "http://elk:9200"
        },
        "file": {
            "enabled": false,
            "path": "logs/logs.txt",
            "interval": "day"
        },
        "seq": {
            "enabled": true,
            "url": "http://seq:5341",
            "apiKey": "secret"
        }
    },

    "metrics": {
        "enabled": true,
        "influxEnabled": false,
        "prometheusEnabled": true,
        "influxUrl": "http://influx:8086",
        "database": "mamey.io",
        "env": "docker",
        "interval": 5
    },
    "rabbitMq": {
        "port": 5672,
        "hostnames": [
            "rabbitmq"
        ]
    },
    "vault": {
        "enabled": true,
        "url": "http://vault:8200",
        "kv": {
            "enabled": true
        },
        "pki": {
            "enabled": false
        }
    }
}