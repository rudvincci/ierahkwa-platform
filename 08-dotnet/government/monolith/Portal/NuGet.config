<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <!--
    Portal-scoped NuGet config for local development builds.
    Release builds use Mamey.* packages (2.0.*) from the local feed.
  -->
  <packageSources>
    <clear />
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="mamey-local" value="http://localhost:4000/v3/index.json" allowInsecureConnections="true" />
  </packageSources>
</configuration>
